<button onclick="calcularOrcamento()">Calcular Orçamento</button>

<div id="resultado"></div>

<script>
function calcularOrcamento() {
  const altura = Number(document.getElementById('altura').value);
  const largura = Number(document.getElementById('largura').value);
  const profundidade = Number(document.getElementById('profundidade').value);
  const material = document.getElementById('material').value;

  if (!altura || !largura || !profundidade) {
    alert('Por favor, preencha todas as medidas.');
    return;
  }

  // Preço por metro cúbico (exemplo)
  const precoMDF = 2500; // Rpor m³
  const precoMDP = 1800; // R por m³

  const volumeM3 = (altura / 100) * (largura / 100) * (profundidade / 100);

  let precoMaterial = material === 'mdf' ? precoMDF : precoMDP;
  let precoFinal = volumeM3 * precoMaterial;
